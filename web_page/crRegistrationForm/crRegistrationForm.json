    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.companyRegistrationRequest ){",
        "\treturn {",
        "\t\t//map companyRegistrationRequest variable to expected task contract input",
        "\t\tcompanyRegistrationRequestInput: {",
        "\t\t\tname: $data.companyRegistrationRequest.name !== undefined ? $data.companyRegistrationRequest.name : null,",
