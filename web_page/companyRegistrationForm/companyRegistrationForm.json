    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.companyRegRequest ){",
        "\treturn {",
        "\t\t//map companyRegRequest variable to expected task contract input",
        "\t\tcompanyRegRequestInput: {",
        "\t\t\tname: $data.companyRegRequest.name !== undefined ? $data.companyRegRequest.name : null,",
        "\t\t\tusername: $data.companyRegRequest.username !== undefined ? $data.companyRegRequest.username : null,",
        "\t\t\tpassword: $data.companyRegRequest.password !== undefined ? $data.companyRegRequest.password : null,",
        "\t\t\taddress: $data.companyRegRequest.address !== undefined ? $data.companyRegRequest.address : null,",
        "\t\t\tpib: $data.companyRegRequest.pib !== undefined ? $data.companyRegRequest.pib : null,",
        "\t\t\tfoundingDate: $data.companyRegRequest.foundingDate !== undefined ? $data.companyRegRequest.foundingDate : null,",
        "\t\t\temail: $data.companyRegRequest.email !== undefined ? $data.companyRegRequest.email : null,",
        "\t\t\ttelephoneNumber: $data.companyRegRequest.telephoneNumber !== undefined ? $data.companyRegRequest.telephoneNumber : null",
